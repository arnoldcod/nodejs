      ul.location_icon
        li
          a(href='#')
            i.fa.fa-map-marker(aria-hidden='true')
          br
          |  kabalagala
        li
          a(href='#')
            i.fa.fa-phone(aria-hidden='true')
          br
          |              (+71) 1234567890
          br
          |  (+71) 1234567890
        li
